/* pages/personCenter/page/myClientele/myClientele.wxss */
.my-clientele{
  height: 100%;
  width: 100%;
}

.clientele-num{
  box-sizing: border-box;
  height: 100rpx;
  width: 100%;
  line-height: 100rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
	background-color: #fff;
}

.clientele-num text{
  color: #fc8c2c;
}

.list{
  margin-top: 10rpx;
}

.list .li{
  height: 128rpx;
  padding:0 30rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
  border-bottom: solid 1rpx #e6e6e6;
}

.list .li image{
  width: 70rpx;
	height: 70rpx;
  border-radius: 50%;
}

.list .li .name{
  flex: 1;
  font-size: 28rpx;
	color: #333;
  margin-left: 15rpx;
} 

.list .li .time{
  font-size: 26rpx;
	color: #666;
} 