/* pages/personCenter/page/withdrawMaterial/withdrawMaterial.wxss */
page{
  background-color: #fff;
}

.remind{
  box-sizing: border-box;
  padding: 0 30rpx;
  width: 100%;
	height: 52rpx;
  line-height: 52rpx;
  color: #fff;
  background-color: rgb(253, 174, 107);
}

.container{
  box-sizing: border-box;
  padding: 0 30rpx;
  width: 100%;
}

.column{
  display: flex;
  align-items: center;
  height: 100rpx;
  width: 100%;	
  font-size: 28rpx;
	color: #808080;
  border-bottom: solid 1rpx #e6e6e6;
}

.column .column-title{
  width: 200rpx;
}

.column .msg{
  flex: 1;
  height: 100%;
}

.column>button{
	width: 178rpx;
	height: 60rpx;
  font-size:24rpx;
  color: #fff;
	background-color: #437ff9;
	border-radius: 8rpx;
}

.send{
  margin-top: 120rpx;
}